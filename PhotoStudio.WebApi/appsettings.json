{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "applicationSettings": {
    "EnableSensitiveDataLogging": false
  },
  "PolicyOptions": {
    "RetryPolicyOptions": {
      "RetryCount": 5,
      "RetryDelay": "00:01:00"
    }
  },
  "CustomExceptionHandlerOptions": {
    "AllwaysReturnOK": false,
    "IncludeDetails": true
  }
}
